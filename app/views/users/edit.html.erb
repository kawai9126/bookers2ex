<h1>編集ページ</h1>
<%= form_with model: @user, url: user_path(@user.id),local:true do |f| %>
<table>
    <tr>
        <th>画像</th>
        <td><%#= f.attachment_field :profile_image %></td>
    </tr>
    <tr>
        <th>タイトル</th>
        <td><%= f.text_field :name %></td>
    </tr>
    <tr>
        <th>詳細</th>
        <td><%= f.text_area :introduction %></td>
        <td><%= f.submit '編集' %></td>
    </tr>
</table>
<% end %>